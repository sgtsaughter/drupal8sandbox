<?php

/**
 * @file
 * Contains pokemon_content_type.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function pokemon_content_type_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the pokemon_content_type module.
    case 'help.page.pokemon_content_type':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('My Awesome Module') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function pokemon_content_type_theme() {
  $theme = [];

  return $theme;
}
